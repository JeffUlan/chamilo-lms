<h4>{{ 'ApprovalForNewAccount'|trans }}</h4>
<ul>
    <li>{{ 'Username'|trans }}: {{ user.getUsername() }}</li>
    {% if is_western_name_order %}
        <li>{{ 'FirstName'|trans }}: {{ user.getFirstName() }}</li>
        <li>{{ 'LastName' }}: {{ user.getLastName() }}</li>
    {% else  %}
        <li>{{ 'LastName' }}: {{ user.getLastName() }}</li>
        <li>{{ 'FirstName' }}: {{ user.getFirstName() }}</li>
    {% endif %}
    <li>{{ 'Email'|trans }}: {{ user.getEmail() }}</li>
    <li>{{ 'Status'|trans }}: {{ user.getStatus() }}</li>
</ul>
<p>{{ 'ManageUser'|trans }}: <a href="{{ manageUrl }}">{{ manageUrl }}</a></p>
