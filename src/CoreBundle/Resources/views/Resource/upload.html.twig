{% extends "@ChamiloTheme/Layout/layout_one_col.html.twig" %}

{% block content %}
  <script>
      $(document).ready(function() {
          $('#fileupload').fileupload({
              formData: {
                  identifier: '{{ identifier }}',
                  type: '{{ type }}'
              }
          });
      });
  </script>

  <input id="fileupload"
         type="file"
         name="files[]"
         data-url="{{ oneup_uploader_endpoint('courses') }}"
         multiple
  />
{% endblock %}
