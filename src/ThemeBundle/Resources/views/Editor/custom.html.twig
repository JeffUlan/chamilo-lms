<head>
{% include "@ChamiloTheme/Layout/head.html.twig" %}
</head>
{% block chamilo_wrap %}
    {% autoescape false %}
        <script>
            $(function () {
                $(".select_to_ckeditor").click(function (e) {
                    e.preventDefault();
                    window.opener.CKEDITOR.tools.callFunction(1, $(this).attr("href"));
                    window.close();
                });
            });
        </script>
    {{ table }}
    {% endautoescape %}
{% endblock %}
