{
    "name": "phpexiftool/phpexiftool",
    "type": "library",
    "description": "Exiftool driver for PHP",
    "keywords": ["metadata","exiftool"],
    "license": "MIT",
    "authors": [
    {
        "name": "Romain Neutron",
        "email": "imprec@gmail.com",
        "homepage": "http://www.lickmychip.com/"
    }
    ],
    "require": {
        "php"                  : ">=5.3.3",
        "doctrine/collections" : "~1.0",
        "doctrine/cache"       : "~1.0",
        "monolog/monolog"      : "~1.3",
        "phpexiftool/exiftool" : "9.15",
        "symfony/process"      : "~2.0"
    },
    "suggest": {
        "jms/serializer"       : "To serialize tags",
        "symfony/yaml"         : "To serialize tags in Yaml format"
    },
    "require-dev": {
        "symfony/css-selector" : "~2.0",
        "symfony/finder"       : "~2.0",
        "symfony/console"      : "~2.0",
        "symfony/dom-crawler"  : "~2.0",
        "symfony/yaml"         : "~2.0",
        "jms/serializer"       : "~0.10",
        "sami/sami"            : "~1.0",
        "silex/silex"          : "~1.0"
    },
    "autoload": {
        "psr-0": {
            "PHPExiftool": "lib"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3-dev"
        }
    }
}
